   "source": [
    "#we implement normalisation using the z metric using the value (x-u)/sigma\n",
    "def normalize_data(data):\n",
    "    mu = np.mean(data, 0)\n",
    "    sigma = np.std(data, 0)\n",
    "    data_norm = (data-mu)/sigma\n",
    "    return data_norm, mu, sigma"
   ]
